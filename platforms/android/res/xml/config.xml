<?xml version='1.0' encoding='utf-8'?>
<widget id="com.AKPCL.SDSv2" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="imeiplugin">
        <param name="android-package" value="com.joandilee.imeiplugin" />
    </feature>
    <feature name="Device">
        <param name="android-package" value="org.apache.cordova.device.Device" />
    </feature>
    <feature name="IMEIGetter">
        <param name="android-package" value="tomloprod.IMEIGetter" />
    </feature>
    <feature name="BarcodeScanner">
        <param name="android-package" value="com.phonegap.plugins.barcodescanner.BarcodeScanner" />
    </feature>
    <allow-intent href="market:*" />
    <name>SDS Operations v2.0</name>
    <description>
        SDS application for KPCT...
    </description>
    <author email="epms_support@krishnapatnamport.com" href="http://www.krishnapatnamport.com">
        Adani - Krishnapatnam Port Company Ltd
    </author>
    <content src="login.html" />
    <icon src="www/img/KPCL.png" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
